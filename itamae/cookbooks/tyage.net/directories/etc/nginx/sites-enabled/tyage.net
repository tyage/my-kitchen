server {
  listen 443 ssl;
  server_name tyage.net www.tyage.net;

  ssl_certificate /etc/letsencrypt/live/tyage.net/fullchain.pem;
  ssl_certificate_key /etc/letsencrypt/live/tyage.net/privkey.pem;

  add_header Strict-Transport-Security "max-age=31536000;";

  access_log /var/log/nginx/tyage.net.access.log;
  error_log  /var/log/nginx/tyage.net.error.log;

  root /var/www/tyage.net/public_html;
  index index.html index.htm;
  charset utf-8;

  location = /robots.txt  { access_log off; log_not_found off; }
  location = /favicon.ico { access_log off; log_not_found off; }
}
